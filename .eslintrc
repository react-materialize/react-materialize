# JavaScript Standard Style Guide: http://standardjs.com
# https://github.com/feross/eslint-config-standard-react
extends:
  - standard
  - standard-react

globals:
  Materialize: true
  React: true
  ReactDOM: true

plugins: ['mocha']

env:
  jquery: true

rules:
  react/jsx-no-bind: [0]
  semi: [2, 'always']
  react/no-unused-prop-types: [0]
  mocha/no-exclusive-tests: [2]
